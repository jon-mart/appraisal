
<table>
  <tr>
    <th></th>
    <th>Name</th>
    <th>salary</th>
    <th>contact</th>
    <th>role</th>
  </tr>
  <% person.each do |f| %>
    <tr>
      <td><%= link_to 'Show', user_path(f) %></td>
      <td><%= f.name %></td>
      <td><%= f.salary %></td>
      <td><%= f.contact %></td>
      <td><%= f.role %></td>
    </tr>
  <% end %>
</table>